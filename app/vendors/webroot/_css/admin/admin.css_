/* Bread Crumb */
.breadCrumb{
	font-family:Arial, Verdana, Helvetica, sans-serif; 
	font-size:14px; 
	font-weight:bold;
	color:#012F63;
	text-decoration:none; 
}

.breadCrumb:hover{
	font-family:Arial, Verdana, Helvetica, sans-serif; 
	font-size:14px; 
	font-weight:bold;
	color:#165D98;
	text-decoration:underline; 
}
/* Links */
.blueLinks{
	font-family:Arial, Verdana, Helvetica, sans-serif; 
	font-size:11px; 
	font-weight:bold;
	color:#012F63;
	text-decoration:none; 
}

.error{
	font-family:Arial, Verdana, Helvetica, sans-serif; 
	font-size:11px; 
	font-weight:normal;
	color:#FF0000;
	text-decoration:none; 
}

.blueLinks:hover{
	font-family:Arial, Verdana, Helvetica, sans-serif; 
	font-size:11px; 
	font-weight:bold;
	color:#165D98;
	text-decoration:underline; 
}
/* Button */
.button{
	font-family:Arial, Verdana, Helvetica, sans-serif; 
	font-size:11px; 
	font-weight:bold;
	color:#165D98;
	border: 1px solid #666;
	width: 100px;
}
/*Large Button */
.largebutton{
	font-family:Arial, Verdana, Helvetica, sans-serif; 
	font-size:11px; 
	font-weight:bold;
	color:#165D98;
	border: 1px solid #666;
	width: 153px;
}
.smallFont{
	font-family:Arial, Verdana, Helvetica, sans-serif; 
	font-size:10px; 
	color:#012F63;
	text-decoration:none; 
}
.mediumFont{
	font-family:Arial, Verdana, Helvetica, sans-serif; 
	font-size:9pt; 
	color:#012F63;
	text-decoration:none; 
}
.mediumFontf{
	font-family:Arial, Verdana, Helvetica, sans-serif; 
	font-size:11px; 
	color:#012F63;
	text-decoration:none; 
}
/* Form */
table{
	font-family:Arial, Verdana, Helvetica, sans-serif; 
	font-size:9pt;
}
.selOpt_par{
	font-weight:bold;
	background-color:#F3F3F3
}
.selOpt_sub{
	font-weight:normal;
	font-size:8pt;
}

.message{
	font-weight:bold;
}
.error-message {
    color: #FF0000;
    float: right;
    width: 500px;
}