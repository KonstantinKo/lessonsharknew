  var el = this.get(el);          el.parentNode.removeChild(el);        }      };      var Event = {        add: function() {          if (window.addEventListener) {            return function(el, type, fn) {              Dom.get(el).addEventListener(type, fn, false);            };          } else if (window.attachEvent) {            return function(el, type, fn) {              var f = function() {                fn.call(Dom.get(el), window.event);              };              Dom.get(el).attachEvent('on' + type, f);            };          }        }()      };//Add location      Event.add(window, 'load', function() {        var i = 1;        Event.add('add-element', 'click', function() {          var el = document.createElement('span');          el.innerHTML = '<fieldset><legend>Specify Location</legend><label for="location_name">Location Name</label><input name="data[UserStoreLocation][t_address_name][]" type="text" placeholder="Home, Work, Office" multiple="multiple" id="UserStoreLocationTAddressNames" /><label for="location_address">Address</label><input name="data[UserStoreLocation][t_address][]" type="text" placeholder="1922 N Sedgwick, Apt 3F" id="UserStoreLocationTAddress" /><label for="location_city">City</label><input name="data[UserStoreLocation][t_city][]" type="text" placeholder="Chicago" id="UserStoreLocationTCity" /><label for="location_state">State</label><input name="data[UserStoreLocation][t_state][]" type="text" placeholder="IL" id="UserStoreLocationTState" /><label for="location_zip">Zip</label><input name="data[UserStoreLocation][t_zip][]" type="text" placeholder="60642" id="UserStoreLocationTZip" /></fieldset>';          Dom.add(el, 'contentLocation');                    Event.add(el, 'click', function(e) {              //Dom.remove(this);            });          });         });       </script> 